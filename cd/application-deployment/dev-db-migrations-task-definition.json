{
  "family": "notification-api-db-migrations-task",
  "executionRoleArn": "arn:aws:iam::437518843863:role/notification-api-ecs-task-execution-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "notification-api-db-migrations",
      "image": "437518843863.dkr.ecr.us-east-2.amazonaws.com/notification_api:ddb34593bfef1810e87b9fb4dbd69957381b5641",
      "cpu": 512,
      "memory": 1024,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "notification-api-log-group",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 6011,
          "hostPort": 6011
        }
      ],
      "command": ["sh", "-c", "flask db upgrade"],
      "environment": [
        {
          "name": "NOTIFY_ENVIRONMENT",
          "value": "development"
        },
        {
          "name": "FLASK_APP",
          "value": "application.py"
        },
        {
          "name": "TWILIO_ACCOUNT_SID",
          "value": "fake"
        },
        {
          "name": "TWILIO_AUTH_TOKEN",
          "value": "fake"
        },
        {
          "name": "TWILIO_FROM_NUMBER",
          "value": "fake"
        }
      ],
      "secrets": [
        {
          "name": "SQLALCHEMY_DATABASE_URI",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/dev/notification-api/database/uri"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "tags": [
    {
      "key": "Stack",
      "value": "application-deployment"
    },
    {
      "key": "Environment",
      "value": "dev"
    },
    {
      "key": "Team",
      "value": "va-notify"
    },
    {
      "key": "ManagedBy",
      "value": "CI"
    }
  ]
}